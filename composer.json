{
  "name": "soukicz/zbozicz",
  "description": "Zboží.cz conversion tracking",
  "minimum-stability": "stable",
  "license": "MIT",
  "keywords": [
    "zbozicz",
    "seznam",
    "zbozi",
    "seznamcz"
  ],
  "authors": [
    {
      "name": "Petr Soukup",
      "email": "soukup@simplia.cz"
    }
  ],
  "require": {
    "lib-curl": "*",
    "php": ">=5.6.0",
    "psr/http-message": "~1.0",
    "guzzlehttp/psr7": "~1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0"
  },
  "suggest": {
    "kdyby/curl-ca-bundle": "Https certificates bundle"
  },
  "autoload": {
    "psr-4": {
      "Soukicz\\Zbozicz\\": "src/"
    },
    "files": [
      "src/exceptions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Soukicz\\TestZbozicz\\": "tests/"
    },
    "files": [
      "src/exceptions.php"
    ]
  }
}
